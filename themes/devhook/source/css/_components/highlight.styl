$base00 = #151515
$base01 = #202020
$base02 = #303030
$base03 = #505050
$base04 = #b0b0b0
$base05 = #d0d0d0
$base06 = #e0e0e0
$base07 = #f5f5f5
$base08 = #ac4142
$base09 = #d28445
$base0A = #f4bf75
$base0B = #90a959
$base0C = #75b5aa
$base0D = #6a9fb5
$base0E = #aa759f
$base0F = #8f5536

.highlight
    margin     0
    color      #333
    background $base07

    table
    tbody
    tr
    td
        width 100% !important
        display block
    pre
        padding    15px
        // padding    15px $base-spacing
        font       80% $mono-font-family
        margin     0
        overflow   auto
        word-break normal

    .variable
        color $base08
    .keyword
        color $base0E
    .constant
    .title
        color $base0D
    .number
        color $base09
    .symbol
    .string
        color $base0B
    .regexp
        color $base0C
    .comment
        color $base04

